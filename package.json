{
  "name": "codewelder",
  "version": "1.0.0",
  "description": "Website or app boilerplate",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose --passWithNoTests",
    "serve": "webpack-dev-server --open --config config/webpack/webpack.dev.js",
    "build": "webpack --env production --config config/webpack/webpack.prod.js",
    "loco": "loco config/loco.config.json",
    "graph": "madge --extensions \"js,jsx\" -i deps.svg \"src/components/\"",
    "report": "webpack-bundle-analyzer ./dist/stats.json"
